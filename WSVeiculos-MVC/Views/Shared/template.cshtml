<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" href="@Url.Content("~/Content/template/")assets/css/main.css" />
    <link rel="shortcut icon" href="@Url.Content("~/Content/template/")favicon.ico" type="image/x-icon">
    <link rel="icon" href="@Url.Content("~/Content/template/")favicon.ico" type="image/x-icon">
    <!--[if lte IE 8]><link rel="stylesheet" href="assets/css/ie8.css" /><![endif]-->

</head>
<body class="homepage" ng-app="app">



    <div>
        @RenderBody()
    </div>


    <!-- Footer -->
    <div id="footer-wrapper" class="wrapper">
        <div class="title">Entre em Contato:</div>
        <div id="footer" class="container">
            <header class="style1">
                <h2>Atendimento Somente com Agendamento Prévio.</h2>
                @*<p>
                        Sed turpis tortor, tincidunt sed ornare in metus porttitor mollis nunc in aliquet.<br />
                        Nam pharetra laoreet imperdiet volutpat etiam consequat feugiat.
                    </p>*@
            </header>
            <hr />
            <div class="row 150%">
                <div class="6u 12u(mobile)">

                    <!-- Contact Form -->
                    <section>
                        <form method="post" action="#">
                            <div class="row 50%">
                                <div class="6u 12u(mobile)">
                                    <input type="text" name="name" id="contact-name" placeholder="Name" />
                                </div>
                                <div class="6u 12u(mobile)">
                                    <input type="text" name="email" id="contact-email" placeholder="Email" />
                                </div>
                            </div>
                            <div class="row 50%">
                                <div class="12u">
                                    <textarea name="message" id="contact-message" placeholder="Mensagem" rows="4"></textarea>
                                </div>
                            </div>
                            <div class="row">
                                <div class="12u">
                                    <ul class="actions">
                                        <li><input type="submit" class="style1" value="Enviar" /></li>
                                        <li><input type="reset" class="style2" value="Apagar" /></li>
                                    </ul>
                                </div>
                            </div>
                        </form>
                    </section>

                </div>
                <div class="6u 12u(mobile)">

                    <!-- Contact -->
                    <section class="feature-list small">
                        <div class="row">
                            <div class="6u 12u(mobile)">
                                <section>
                                    <h3 class="icon fa-home">Endereço</h3>
                                    <p>
                                        Escritório Virtual Shopping Total, Anexo 2 - Av. Cristóvão Colombo, 545 - Floresta, Porto Alegre - RS, 90560-003
                                    </p>
                                </section>
                            </div>
                            <div class="6u 12u(mobile)">
                                <section>
                                    <h3 class="icon fa-comment">Redes Sociais</h3>
                                    <p>
                                        <a href="#">WSVeículos.corp</a><br />
                                        <a href="#">facebook.com/WSVeículos</a>
                                    </p>
                                </section>
                            </div>
                        </div>
                        <div class="row">
                            <div class="6u 12u(mobile)">
                                <section>
                                    <h3 class="icon fa-envelope">Email</h3>
                                    <p>
                                        <a href="#">wsveículos@yahoo.com.br</a>
                                    </p>
                                </section>
                            </div>
                            <div class="6u 12u(mobile)">
                                <section>
                                    <h3 class="icon fa-phone">Telefone</h3>
                                    <p>
                                        (51)3335-5841
                                    </p>
                                </section>
                            </div>
                        </div>
                    </section>

                </div>
            </div>
            <hr />
        </div>
        <div id="copyright">
            <ul>
                <li>&copy; Design:</li>
                <li><a href="">Oséias Freitas</a></li>
            </ul>
        </div>
    </div>

    <!-- Scripts -->

    <script src="@Url.Content("~/assets/")/js/jquery.min.js"></script>
    <script src="@Url.Content("~/Scripts/")node_modules/angular/angular.min.js"></script>
    <script src="@Url.Content("~/assets/")/js/jquery.dropotron.min.js"></script>
    <script src="@Url.Content("~/assets/")/js/skel.min.js"></script>
    <script src="@Url.Content("~/assets/")/js/skel-viewport.min.js"></script>
    <script src="@Url.Content("~/assets/")/js/util.js"></script>
    <!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
    <script src="@Url.Content("~/assets/")/js/main.js"></script>

    <script src="@Url.Content("~/Scripts/")node_modules/angular-route/angular-route.min.js"></script>

    <script src="@Url.Content("~/Scripts/")/app.js"></script>
    <script src="@Url.Content("~/Scripts/")/config.js"></script>
    <script src="@Url.Content("~/Scripts/")/controllers/mainControl.js"></script>
    <script src="@Url.Content("~/Scripts/")/controllers/finControl.js"></script>
    <script src="@Url.Content("~/Scripts/")/controllers/contactControl.js"></script>
</body>
</html>
