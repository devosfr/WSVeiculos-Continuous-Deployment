
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SelectedVal</title>
</head>
<body>
    <div id="gridSelect"> 

        <div class="container">
            <h2>Basic Table</h2>
            <p>The .table class adds basic styling (light padding and only horizontal dividers) to a table:</p>
            <table class="table">
                <thead>
                    <tr>
                        <th>Valor Selecionado</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@ViewBag.Valor</td>
                    </tr>
                </tbody>
            </table>
        </div>



    </div>
</body>
</html>
