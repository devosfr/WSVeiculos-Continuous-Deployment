                </ul>
            </nav>


        </div>
    </div>

    <div class="content contentContact">

        <h1>Contato</h1>

        <form class="form-horizontal" id="form1">
            <input type="hidden" id="metodo" value="formulario-ajax">
            <div class="form-group">
                <label class="control-label col-sm-2" for="nome">Nome:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="nome" placeholder="Nome">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2" for="sobrenome">Sobrenome:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="sobrenome" placeholder="Sobrenome">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2" for="tel">Telefone:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="tel" placeholder="Telefone">
                </div>
                <div class="col-sm-10">
                    <label class="control-label col-sm-2" for="tel">Marca:</label>
                    <select id="selectContact">
                        <option value=""></option>
                        @foreach (var item in Model)
                        {
                            <option value="@item">@item</option>
                        }
                    </select>

                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2" for="msn">Mensagem:</label>
                <div class="col-sm-10">
                    <textarea name="" id="msn" cols="30" rows="10"></textarea>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <h4>Receber Anúncios em meu por e-mail</h4>
                    <div class="checkbox">
                        <label><input type="radio" value="1" name="checkboxAnuncios" class="checkboxAnuncios" checked="checked">Sim</label>
                    </div>
                    <div class="checkbox">
                        <label><input type="radio" value="0" name="checkboxAnuncios">Não</label>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-default" id="btnSubmit">Enviar Formulário</button>
                </div>
            </div>
        </form>


    </div>


</div>


<div>
    @Html.Partial("SelectedVal")
</div>